{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "eqeqeq": "warn",
    "no-empty-function": "error",
    "no-return-await": "off",
    "array-callback-return": "off",
    "linebreak-style": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "jsx-a11y/href-no-hash": "off",
    "no-script-url": "off",
    "jsx-a11y/anchor-has-content": "off"
  },
  "extends": "react-app",
  "env": {
    "browser": true,
    "node": true
  }
}
